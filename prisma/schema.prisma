generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model users {
  id            Int    @id @default(autoincrement())
  email         String @db.VarChar(255)
  username      String @db.VarChar(255)
  password_hash String @db.VarChar(255)
  name          String @db.VarChar(255)
}
